# PgBouncer Environment Variables
# Copy this file to .env and update with your actual password

# PostgreSQL credentials (your existing database)
POSTGRES_PASSWORD=your_postgres_password_here

# PgBouncer port (Django will connect to this instead of 5432)
PGBOUNCER_PORT=6432

# Keep existing PostgreSQL for direct access if needed
POSTGRES_PORT=5432
POSTGRES_HOST=localhost
POSTGRES_USER=postgres
POSTGRES_DB=postgres
